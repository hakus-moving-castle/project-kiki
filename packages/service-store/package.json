{
  "name": "@repo/service-store",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development pnpm with-env run-p dev:*",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:tsx": "tsx --watch ./src/app.ts",
    "build": "cross-env NODE_ENV=production pkgroll",
    "db": "run-p db:*",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "studio": "prisma studio",
    "lint": "tsc && eslint .",
    "with-env": "dotenv -e ./.env --"
  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "@repo/util-logger": "workspace:*",
    "body-parser": "^1.20.2",
    "drizzle-orm": "^0.32.1",
    "express": "^4.19.2",
    "prisma-client": "^0.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.42",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "pkgroll": "^2.4.2",
    "prisma": "^5.17.0",
    "tsx": "^4.16.5",
    "typescript": "catalog:"
  }
}
