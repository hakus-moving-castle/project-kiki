{
	"display": "Nest.js",
	"extends": "./base.json",
	"compilerOptions": {
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"outDir": "${configDir}/dist",
		"baseUrl": "${configDir}/",
		"rootDir": "${configDir}/",
		"incremental": true,
		"forceConsistentCasingInFileNames": true
	}
}
