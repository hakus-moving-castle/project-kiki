{
  "name": "project-kiki",
  "private": true,
  "description": "purchasing agent management",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen:library": "turbo gen workspace --type package --copy @template/library",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.1",
    "@commitlint/cz-commitlint": "^18.6.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "turbo": "^2.0.9"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20"
  }
}
